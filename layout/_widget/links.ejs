<% if (site.posts.length){ %>
  <div class="widget tag">
    <h3 class="title"><%= __('links') %></h3>
      <ul class="entry">
        <% for (var i in theme.links){ %>
          <li>
            <a href="<%- theme.links[i] %>" target="_blank"><%= i %></a>
          </li>
        <% } %>
      </ul>
  </div>
<% } %>